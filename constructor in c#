using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


namespace SoloLearn
{
	class Program
	{
        class Person{
            string name;
            public Person(string nm)
            {
              name=nm;  
             
            }
            public string getname(){
                return name;
            }
        }
            static void Main(string[] args){
                Person P =new Person("shreya!");
                Console.WriteLine(P.getname());

            }
            
    }
}
