using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


namespace SoloLearn
{
	class Program
	{
        static int sum(int x,int y=7){
            int result=0;
            for(int i=0;i<=y;i++){
                result +=x;

            }
            return result;

        }
		static void Main(string[] args)
		{   
            Console.WriteLine(sum(x:4,y:8));

			
		}
	}
}
