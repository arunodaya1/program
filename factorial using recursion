using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


namespace SoloLearn
{
	class Program
	{
        static int fact(int num){
            if (num==1){
                return 1;
            }
            return num*fact(num-1);
        }
		static void Main(string[] args)
		{
            Console.WriteLine(fact(5));
			
		}
	}
}
