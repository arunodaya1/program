#include <stdio.h>

int main() {
    int a[2][2]={
        {3,6},{5,8}

    };
    printf("element 2 in row 2 is %d\n",a[1][1]);
    printf("element 1 in row 2 is %d\n",a[1][0]);
    printf("element 1 in row 1 is %d\n",a[0][0]);
    printf("element 2 in row 1 is %d\n",a[0][1]);
    a[0][1]=56;
    printf("element 1 in row 1 is %d\n",a[0][1]);
    int j,k;
    int sum=0;
    for(j=0;j<2;j++){
        for(k=0;k<2;k++){
        sum+=a[j][k];    

            printf("%d\n",a[j][k]);
        }
    }
    printf("total element is %d",sum);
    return 0;
}
